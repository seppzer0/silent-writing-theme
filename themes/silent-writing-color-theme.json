	"editorBracketHighlight.foreground1": "#d2d2d2",
    "editorBracketHighlight.foreground2": "#d2d2d2",
    "editorBracketHighlight.foreground3": "#d2d2d2",
    "extensionButton.prominentBackground": "#d2d2d2",
    "extensionButton.prominentForeground": "#d8d8d8",
	"editorGroup.border": "#040404",
	"editorGroup.dropBackground": "#040404",
	"editorGroup.emptyBackground": "#040404",
	"editorGroupHeader.noTabsBackground": "#040404",
	"editorGroupHeader.tabsBackground": "#040404",
	"editorGroupHeader.tabsBorder": "#040404",
	"editorIndentGuide.activeBackground1": "#101010",
	"editorIndentGuide.background1": "#101010",
	"editorRuler.foreground": "#222",
	"editorWhitespace.foreground": "#040404",
	"input.placeholderForeground": "#A6A6A6",
	"list.activeSelectionBackground": "#454545",
	"list.activeSelectionIconForeground": "#FFF",
	"list.dropBackground": "#383B3D",
	"menu.background": "#040404",
	"menu.foreground": "#CCCCCC",
	"menu.selectionBorder": "#818181",
	//"menu.border": "#818181",
	"menu.selectionForeground": "#d4d4d4",
	"menu.selectionBackground": "#474747",
	"menubar.selectionBackground": "#333333",
	"menubar.selectionBorder": "#333333",
	"menubar.selectionForeground": "#d2d2d2",
	"ports.iconRunningProcessForeground": "#CCCCCC",
	"scrollbarSlider.activeBackground": "#fff4",
	"scrollbarSlider.background": "#fff2",
	"scrollbarSlider.hoverBackground": "#fff3",
	"sideBar.background": "#040404",
	"sideBar.border": "#040404",
